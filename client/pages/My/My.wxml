<view class='View_Bg'>
  <view class='imgs'>
    <open-data type="userAvatarUrl" id='userImg'></open-data>
  </view>
</view>
<view class='view_Name'>
  <open-data type="userNickName" class="userName"></open-data>
</view>

<view class='taKing'>
  <text class='this_month'>本月打车</text>
  <view class='shuxian'></view>
  <text class='this_year'>今年打车</text>
  <view class='shuxian2'></view>
  <text class='this_all'>总共打车</text>

  <text class='month_conunt'>{{OrderCount.Month}}</text>
  <text class='year_conunt'>{{OrderCount.yearcount}}</text>
  <text class='all_conunt'>{{OrderCount.sumcount}}</text>
</view>

<view class='My_collect' bindtap='My_collect'>
  <text class='My_collect-t'>我的收藏</text>
</view>
<view class='My_taKing' bindtap='OpenOrder'>
  <text class='My_taKing-t'>我的打车记录</text>
</view>
<view class='My_help' bindtap='OpenHelp'>
  <text class='My_help-t'>帮助</text>
</view>
<view class='class_arrow1'>
  <text class='iconfont .icon-arrow-right-copy-copy-copy'></text>
</view>
<view class='class_arrow2'>
  <text class='iconfont .icon-arrow-right-copy-copy-copy'></text>
</view>
<view class='class_arrow3'>
  <text class='iconfont .icon-arrow-right-copy-copy-copy'></text>
</view>


